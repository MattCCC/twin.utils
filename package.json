{
  "version": "1.0.1",
  "license": "MIT",
  "name": "twin.utils",
  "author": "Matt Czapli≈Ñski <deindesign.pl@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/MattCCC/twin.utils.git"
  },
  "main": "dist/browser/index.mjs",
  "typings": "dist/index.d.ts",
  "keywords": [
    "axios-api",
    "axios-api-handler",
    "twin.utils",
    "api",
    "api-handler",
    "browser",
    "node"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "tsup src/index.ts --watch",
    "build": "node ./scripts/build.js",
    "build:browser": "tsup src/index.ts --dts --format esm,iife --sourcemap --env.NODE_ENV production --minify --dts-resolve",
    "build:node": "tsup src/index.ts --dts --format cjs --sourcemap --env.NODE_ENV production --minify",
    "test": "jest --forceExit",
    "lint": "eslint --ext .js,.ts",
    "release": "npm version patch && git push --tags",
    "prepare": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/browser/index.mjs",
  "size-limit": [
    {
      "path": "dist/browser/index.mjs",
      "limit": "10 KB"
    },
    {
      "path": "dist/browser/index.global.js",
      "limit": "50 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "@types/jest": "^29.2.1",
    "eslint": "^8.26.0",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "rollup-plugin-bundle-imports": "^1.5.1",
    "size-limit": "^8.1.0",
    "ts-jest": "^29.0.3",
    "tslib": "^2.4.1",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "twin.macro": "^2.8.2"
  }
}
